{
  "name": "friendlydev/microsoft-correlationvector-php",
  "description": "Microsoft Correlation Vector.",
  "keywords": [
    "microsoft",
    "correlation-vector",
    "correlation",
    "vector",
    "ms-cv"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1"
  },
  "require-dev": {
    "pestphp/pest": "^1.21"
  },
  "autoload": {
    "psr-4": {
      "MicrosoftCV\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "platform": {
      "php": "8.1"
    },
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
